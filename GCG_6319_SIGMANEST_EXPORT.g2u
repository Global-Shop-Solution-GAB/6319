'F.Data.DataTable.CreateFromSQL("Temp","con",V.Local.sSql)
'F.Data.DataView.Create("Temp","VTemp",22,"BDATE not is null","")
'F.Data.Dictionary.CreateFromDataView("BDate","Temp","VTemp","Jss","BDate")
'F.Data.DataView.Close("Temp","VTemp")
'F.Data.DataTable.Close("Temp")
''F.Data.Dictionary.CreateFromSQL("BDate","con",V.Local.sSql)
